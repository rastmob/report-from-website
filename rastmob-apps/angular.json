{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "report-from-website": {
      "projectType": "library",
      "root": "projects/report-from-website",
      "sourceRoot": "projects/report-from-website/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/report-from-website/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/report-from-website/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/report-from-website/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/report-from-website/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "7b0e7c34-9621-4c97-ab0b-faaf050c2c28"
  }
}
